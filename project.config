[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Panagiotis Garefalakis (pgaref)
[access "refs/for/refs/*"]
	read = group user/Panagiotis Garefalakis (pgaref)
	push = group user/Panagiotis Garefalakis (pgaref)
	label-Code-Review = -2..+2 group user/Panagiotis Garefalakis (pgaref)
	label-Verified = -1..+1 group user/Panagiotis Garefalakis (pgaref)
	rebase = group user/Panagiotis Garefalakis (pgaref)
	removeReviewer = group user/Panagiotis Garefalakis (pgaref)
	submit = group user/Panagiotis Garefalakis (pgaref)
[access "refs/heads/*"]
	read = group user/Panagiotis Garefalakis (pgaref)
	create = group user/Panagiotis Garefalakis (pgaref)
	push = +force group user/Panagiotis Garefalakis (pgaref)
	pushMerge = group user/Panagiotis Garefalakis (pgaref)
[access "refs/tags/*"]
	push = +force group user/Panagiotis Garefalakis (pgaref)
	pushTag = group user/Panagiotis Garefalakis (pgaref)
	pushSignedTag = group user/Panagiotis Garefalakis (pgaref)
